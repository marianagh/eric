<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.itson.seam" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".rebaActivity"
            android:label="@string/title_activity_reba"
            android:parentActivityName=".MainActivity"
            >
        </activity>
        <activity android:name=".rulaActivity"
            android:label="@string/title_activity_rula"
            android:parentActivityName=".MainActivity"
            >
        </activity>
        <activity android:name=".consultarActivity"
            android:label="@string/title_activity_consultar"
            android:parentActivityName=".MainActivity"
            >
        </activity>
    </application>

</manifest>
